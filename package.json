{
  "name": "reactboilerplate",
  "version": "1.0.0",
  "description": "A react boilerplate built with react16 and webpack4 with emphasis on build and integration process and project structure for scalable apps",
  "author": "ayman smimah <aymansmimah@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --inline --config build/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production node build/build-prod.js",
    "lint": "eslint --ext .js,.jsx --fix .",
    "prettify": "prettier --write 'src/**/*.{js,jsx,css,scss}'",
    "predeploy": "yarn build",
    "deploy": "netlify deploy --prod",
    "deploy_dev": "netlify deploy",
    "CI": "gulp CI",
    "build:watch": "gulp cd_watch",
    "gulp:build": "gulp build"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7",
    "react-paginate": "^6.5.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.0.0-beta.33",
    "@babel/preset-react": "^7.0.0-beta.33",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "deepmerge": "^4.2.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-remove-code": "^3.0.4",
    "gulp-watch": "^5.0.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^5.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.3",
    "semver": "^7.3.2",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^5.1.1",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 6.0.0"
  }
}
